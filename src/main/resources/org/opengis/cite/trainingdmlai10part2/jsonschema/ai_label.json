{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://localhost:8000/ai_label.json",
    "title": "AI_AbstractLabel",
    "type": "object",
    "required": [
        "type"
    ],
    "properties": {
        "type": {
            "type": "string",
            "enum": [
                "AI_AbstractLabel"
            ]
        },
        "confidence": {
            "type": "number",
            "default": 1.0,
            "minimum": 0.0,
            "maximum": 1.0
        },
        "isNegative": {
            "type": "boolean",
            "default": false
        }
    }
}