{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://localhost:8000/md_dimension.json",
    "title": "MD_Dimension",
    "type": "object",
    "required": [
        "dimensionName",
        "dimensionSize"
    ],
    "properties": {
        "dimensionName": {
            "type": "string"
        },
        "dimensionSize": {
            "type": "integer"
        },
        "resolution": {
            "type": "string"
        },
        "dimensionTitle": {
            "type": "string"
        },
        "dimensionDescription": {
            "type": "string"
        }
    }
}