{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://localhost:8000/md_medium.json",
    "title": "MD_Medium",
    "type": "object",
    "properties": {
        "name": {
            "$ref": "./ci_citation.json"
        },
        "density": {
            "type": "number"
        },
        "densityUnits": {
            "type": "string"
        },
        "volumes": {
            "type": "integer"
        },
        "mediumFormat": {
            "type": "array",
            "items": {
                "type": "string"
            }
        },
        "mediumNote": {
            "type": "string"
        },
        "identifier": {
            "$ref": "./md_identifier.json"
        }
    }
}