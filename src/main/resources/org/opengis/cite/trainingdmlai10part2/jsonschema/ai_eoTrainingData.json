{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://localhost:8000/ai_eoTrainingData.json",
    "title": "AI_EOTrainingData",
    "type": "object",
    "required": [
        "type",
        "id",
        "labels",
        "dataURL"
    ],
    "properties": {
        "type": {
            "type": "string",
            "enum": [
                "AI_EOTrainingData"
            ]
        },
        "datasetId": {
            "type": "string"
        },
        "dataSources": {
            "type": "array",
            "items": {
                "$ref": "./ci_citation.json"
            }
        },
        "id": {
            "type": "string"
        },
        "numberOfLabels": {
            "type": "integer"
        },
        "dataURL": {
            "type": "array",
            "items": {
                "type": "string",
                "format": "uri-reference"
            },
            "minItems": 1
        },
        "extent": {
            "$ref": "./ex_extent.json"
        },
        "dataTime": {
            "type": "array",
            "items": {
                "$ref": "./dateTime.json"
            }
        },
        "labels": {
            "type": "array",
            "items": {
                "oneOf": [
                    {
                        "$ref": "./ai_label.json"
                    },
                    {
                        "$ref": "./ai_sceneLabel.json"
                    },
                    {
                        "$ref": "./ai_objectLabel.json"
                    },
                    {
                        "$ref": "./ai_pixelLabel.json"
                    }
                ]
            },
            "minItems": 1
        },
        "labeling": {
            "type": "array",
            "items": {
                "$ref": "./ai_labeling.json"
            }
        },
        "trainingType": {
            "$ref": "./ai_trainingTypeCode.json"
        },
        "quality": {
            "type": "array",
            "items": {
                "$ref": "./dataQuality.json"
            }
        }
    }
}