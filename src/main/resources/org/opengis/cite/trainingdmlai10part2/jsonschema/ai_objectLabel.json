{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://localhost:8000/ai_objectLabel.json",
    "title": "AI_ObjectLabel",
    "type": "object",
    "required": [
        "type",
        "object",
        "class"
    ],
    "properties": {
        "type": {
            "type": "string",
            "enum": [
                "AI_ObjectLabel"
            ]
        },
        "confidence": {
            "type": "number",
            "default": 1.0,
            "minimum": 0.0,
            "maximum": 1.0
        },
        "isNegative": {
            "type": "boolean",
            "default": false
        },
        "bboxType": {
            "type": "string"
        },
        "class": {
            "type": "string"
        },
        "object": {
            "$ref": "https://geojson.org/schema/Feature.json"
        },
        "dateTime": {
            "$ref": "./dateTime.json"
        }
    }
}