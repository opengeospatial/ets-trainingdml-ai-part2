{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "ci_organisation.json",
  "title": "CI_Organisation",
  "type": "object",
  "properties": {
    "name": {
      "type": "string"
    },
    "contactInfo": {
      "type": "array",
      "items": {
        "$ref": "ci_contact.json"
      }
    },
    "partyIdentifier": {
      "type": "array",
      "items": {
        "$ref": "md_identifier.json"
      }
    },
    "logo": {
      "type": "array",
      "items": {
        "$ref": "md_browseGraphic.json"
      }
    },
    "individual": {
      "type": "array",
      "items": {
        "$ref": "ci_individual.json"
      }
    }
  }
}