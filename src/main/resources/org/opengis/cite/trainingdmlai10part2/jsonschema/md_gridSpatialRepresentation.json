{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "md_gridSpatialRepresentation.json",
  "title": "MD_GridSpatialRepresentation",
  "type": "object",
  "required": [
    "numberOfDimensions",
    "cellGeometry",
    "transformationParameterAvailability"
  ],
  "properties": {
    "scope": {
      "$ref": "md_scope.json"
    },
    "numberOfDimensions": {
      "type": "integer"
    },
    "axisDimensionProperties": {
      "type": "array",
      "items": {
        "$ref": "md_dimension.json"
      }
    },
    "cellGeometry": {
      "type": "string"
    },
    "transformationParameterAvailability": {
      "type": "boolean"
    }
  }
}