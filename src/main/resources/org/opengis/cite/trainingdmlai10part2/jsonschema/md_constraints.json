{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "md_constraints.json",
  "title": "MD_Constraints",
  "type": "object",
  "properties": {
    "useLimitation": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "constraintApplicationScope": {
      "$ref": "md_scope.json"
    },
    "graphic": {
      "type": "array",
      "items": {
        "$ref": "md_browseGraphic.json"
      }
    },
    "reference": {
      "type": "array",
      "items": {
        "$ref": "ci_citation.json"
      }
    },
    "releasability": {
      "$ref": "md_releasability.json"
    },
    "responsibleParty": {
      "type": "array",
      "items": {
        "$ref": "ci_responsibility.json"
      }
    }
  }
}