{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "ci_individual.json",
  "title": "CI_Individual",
  "type": "object",
  "properties": {
    "name": {
      "type": "string"
    },
    "contactInfo": {
      "type": "array",
      "items": {
        "$ref": "ci_contact.json"
      }
    },
    "partyIdentifier": {
      "type": "array",
      "items": {
        "$ref": "md_identifier.json"
      }
    },
    "positionName": {
      "type": "string"
    }
  }
}