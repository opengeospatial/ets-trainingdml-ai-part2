{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "ai_sceneLabel.json",
  "title": "AI_SceneLabel",
  "type": "object",
  "required": [
    "type",
    "class"
  ],
  "properties": {
    "type": {
      "type": "string",
      "enum": [
        "AI_SceneLabel"
      ]
    },
    "class": {
      "type": "string"
    },
    "confidence": {
      "type": "number",
      "default": 1.0,
      "minimum": 0.0,
      "maximum": 1.0
    },
    "isNegative": {
      "type": "boolean",
      "default": false
    }
  }
}