{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://localhost:8000/md_band.json",
    "title": "MD_Band",
    "type": "object",
    "properties": {
        "sequenceIdentifier": {
            "$ref": "./memberName.json"
        },
        "description": {
            "type": "string"
        },
        "name": {
            "type": "array",
            "items": {
                "$ref": "./md_identifier.json"
            }
        },
        "maxValue": {
            "type": "number"
        },
        "minValue": {
            "type": "number"
        },
        "units": {
            "type": "string"
        },
        "scaleFactor": {
            "type": "number"
        },
        "offset": {
            "type": "number"
        },
        "meanValue": {
            "type": "number"
        },
        "numberOfValues": {
            "type": "integer"
        },
        "standardDeviation": {
            "type": "number"
        },
        "otherPropertyType": {
            "type": "string"
        },
        "otherProperty": {
            "type": "string"
        },
        "bitsPerValue": {
            "type": "integer"
        },
        "rangeElementDescription": {
            "type": "array",
            "items": {
                "$ref": "./mi_rangeElementDescription.json"
            }
        },
        "boundMax": {
            "type": "number"
        },
        "boundMin": {
            "type": "number"
        },
        "boundUnits": {
            "type": "string"
        },
        "peakResponse": {
            "type": "number"
        },
        "toneGradation": {
            "type": "integer"
        }
    },
    "dependentRequired": {
        "boundMax": [
            "boundUnits"
        ],
        "boundMin": [
            "boundUnits"
        ]
    }
}